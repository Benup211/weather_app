{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "es6",
      "dom"
    ],
    "jsx": "react-native",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "app/**/*",
    "src/**/*",
    "@types/**/*",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}